{
  "dashboard": {
    "id": null,
    "title": "0G Galileo Beacon Chain Block Signing Timeline",
    "tags": ["cosmos", "0g", "galileo", "beacon", "timeline"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Beacon Chain Block Signing Timeline (Based on Previous Block)",
        "type": "statetable",
        "targets": [
          {
            "expr": "og_galileo_validator_beacon_block_signed{validator=\"validator1\"}",
            "legendFormat": "Validator1 - Block {{block_height}}",
            "refId": "A"
          },
          {
            "expr": "og_galileo_validator_beacon_block_signed{validator=\"validator2\"}",
            "legendFormat": "Validator2 - Block {{block_height}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "displayMode": "color-background",
              "align": "auto"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "text": "Missed"
                  },
                  "1": {
                    "color": "green",
                    "text": "Signed"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 12,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "options": {
          "showValue": "auto",
          "sortBy": "Time",
          "sortDesc": true
        }
      },
      {
        "id": 2,
        "title": "Validator Status Timeline",
        "type": "statetable",
        "targets": [
          {
            "expr": "og_galileo_validator_status{validator=\"validator1\"}",
            "legendFormat": "Validator1 Status",
            "refId": "A"
          },
          {
            "expr": "og_galileo_validator_status{validator=\"validator2\"}",
            "legendFormat": "Validator2 Status",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "displayMode": "color-background",
              "align": "auto"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "text": "Inactive"
                  },
                  "1": {
                    "color": "green",
                    "text": "Active"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 12
        },
        "options": {
          "showValue": "auto",
          "sortBy": "Time",
          "sortDesc": true
        }
      },
      {
